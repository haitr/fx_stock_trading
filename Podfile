# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

# Comment the next line if you don't want to use dynamic frameworks
use_frameworks!

target 'fx-native-app' do
  # Rx libs
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxGesture'
  # Network layer
  pod 'Moya'
  # Auto Layout
  pod 'SnapKit'
  # PopOver
  pod "PopoverSwift"
end

target 'fx-native-app-test' do
  pod 'RxBlocking'
  pod 'RxTest'
end
